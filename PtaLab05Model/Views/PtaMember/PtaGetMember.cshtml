@model List<PtaLab05Model.Models.DataModels.PtaMember>
@{
    var sinhVien = ViewBag.PtaMember as PtaLab05Model.Models.DataModels.PtaMember;
    ViewData["Title"] = "Thông tin sinh viên & danh sách member";
}

<h2>Thông tin sinh viên</h2>
<div class="alert alert-info">
    <p><strong>ID:</strong> @sinhVien.PtaMemberId</p>
    <p><strong>Username:</strong> @sinhVien.PtaUserName</p>
    <p><strong>Fullname:</strong> @sinhVien.PtaFullName</p>
    <p><strong>Password:</strong> @sinhVien.PtaPassword</p>
    <p><strong>Email:</strong> @sinhVien.PtaEmail</p>
</div>

<h2>Danh sách Members</h2>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>UserName</th>
            <th>FullName</th>
            <th>Password</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var member in Model)
        {
            <tr>
                <td>@member.PtaMemberId</td>
                <td>@member.PtaUserName</td>
                <td>@member.PtaFullName</td>
                <td>@member.PtaPassword</td>
                <td>@member.PtaEmail</td>
            </tr>
        }
    </tbody>
</table>
